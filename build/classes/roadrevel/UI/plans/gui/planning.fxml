<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="219.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="roadrevel.circuit_touristique.controllers.PlanningController">
   <children>
      <HBox prefHeight="51.0" prefWidth="560.0">
         <children>
            <Label fx:id="planningJourLabel" text="Label" wrapText="true">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font></Label>
            <Label fx:id="planningNameLabel" text="Label" wrapText="true">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <Label fx:id="planningDescriptionLabel" text="Label" wrapText="true">
         <font>
            <Font name="Arial" size="18.0" />
         </font></Label>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
